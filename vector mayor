#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;
void main(){
	int n,numeros[100];
	int mayor=0;
	cout<<"ingrese el numero de elementos del vector: ";
	cin>>n;
	
	for(int i=0;i<n;i++){
		cout<<"ingrese el elemento: ["<<i<<"]: ";
		cin>>numeros[i];

		if(numeros[i]>mayor){
			mayor=numeros[i];
			}
	}
			cout<<"Tu numero mayor es: "<<mayor<<endl;
getch();
}
