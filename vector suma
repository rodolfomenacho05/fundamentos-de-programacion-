// rodolfo80.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;
void main(){
	int n,numeros[10];
	int suma=0;
	cout<<"ingrese el numero de elementos del vector: ";
	cin>>n;
	
	for(int i=0;i<n;i++){
		cout<<"ingrese el elemento: ["<<i<<"]: ";
		cin>>numeros[i];
	suma=suma+numeros[i];
	}
	
	cout<<"La suma de los elementos del vector es: "<<suma<<endl;

	getch();
}
